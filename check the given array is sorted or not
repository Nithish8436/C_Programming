//check the given array is sort or not 

#include <stdio.h>
#include <stdbool.h>
int main(){
    int n=5;
    int arr[100];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    
    for(int i=1;i<n;i++){
        if(arr[i]<arr[i-1]){
            printf("False");
            return 0;
        }
    }
    printf("True");
}
